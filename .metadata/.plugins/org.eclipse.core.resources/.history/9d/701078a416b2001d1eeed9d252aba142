package Matrix;
import java.util. Scanner;

public class SumofMatrix {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the Number");
		int n=sc.nextInt();
		
		for(int i=0;i<=n;i++) {
			for(int j=n;j>1;j--)
			{
			  System.out.print(" ");
			  
			}
			for(int k=n;k>=1;k-- )
			{
				System.out.print(k);
			}
			System.out.println();
		}
		

	}

}
